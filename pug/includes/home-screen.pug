mixin home
  .container.home-screen#home
    h1(class="text-center fs-5 pt-3 pb-3") Welcome To Server Settings Manger
    ul 
      each item in navsNames
        li(class=item.class)
          button 
            span= item.name
            i(class="fa-solid fa-angle-right")
      li.discord
        a(href="https://discord.gg/jNeqw9zmY6",
        class="d-flex justify-content-between align-items-center w-100") Discord
          i(class="fa-solid fa-angle-right")